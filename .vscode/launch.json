{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ARES example 1",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/ares",
            "args": [
                "pipeline",
                "--workflow",
                "${workspaceFolder}/examples/workflow/workflow_example_1.json",
                "--output",
                "${workspaceFolder}/examples/output/",
                "--log-level",
                "10"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        }
    ]
}
