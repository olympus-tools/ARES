{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ARES example 1",
            "type": "debugpy",
            "request": "launch",
            "python": "${workspaceFolder}/.venv/bin/python",
            "module": "ares",
            "args": [
                "pipeline",
                "--workflow",
                "${workspaceFolder}/examples/workflow/workflow_example_1.json",
                "--output",
                "${workspaceFolder}/examples/output/",
                "--log-level",
                "10"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "prepare-simunit-all",
            "postDebugTask": "clean"
        },
